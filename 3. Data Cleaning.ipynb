    "df_houses.drop(df_houses[df_houses[\"beds\"] == \"窶能"].index, inplace = True)\n",
    "df_houses.drop(df_houses[df_houses[\"baths\"] == \"窶能"].index, inplace = True)\n",
    "df_houses.drop(df_houses[df_houses[\"baths\"] == \"2,408 Sq. Ft.\"].index, inplace = True)\n",
    "df_houses.drop(df_houses[df_houses[\"house_sqft\"] == \"窶能"].index, inplace = True)\n",
    "df_houses.drop(df_houses[df_houses[\"house_sqft\"] == \"\"].index, inplace = True)\n",
    "df_houses.drop(df_houses[df_houses[\"lotsize\"] == \"\"].index, inplace = True)\n",
    "df_houses.drop(df_houses[df_houses[\"lotsize\"] == \"\"].index, inplace = True)\n",
    "df_houses.drop(df_houses[df_houses[\"year_built\"] == \"窶能"].index, inplace = True)"

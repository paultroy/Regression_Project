   "metadata": {},
   "outputs": [],
   "source": [
    "# Remove houses that either have incorrect sale price information. Remove outliers that have...\n",
    "# ...sale prices greater than $1.5M or lot larger than 1 acre\n",
    "\n",
    "df_houses.drop(df_houses[df_houses[\"sale_price\"] < 15000].index, inplace = True)\n",
    "df_houses.drop(df_houses[df_houses[\"sale_price\"] > 1500000].index, inplace = True)\n",
    "df_houses.drop(df_houses[df_houses[\"lotsize\"] > 43560].index, inplace = True)"
   ]

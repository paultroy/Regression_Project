   "source": [
    "# Strip column values out of list format\n",
    "\n",
    "df_houses[\"zipcode\"] = [i[0] for i in df_houses[\"zipcode\"]]\n",
    "df_houses[\"sale_price\"] = [i[0] for i in df_houses[\"sale_price\"]]\n",
    "df_houses[\"house_sqft\"] = [''.join(i) for i in df_houses[\"house_sqft\"]]\n",
    "df_houses[\"lotsize\"] = [''.join(i) for i in df_houses[\"lotsize\"]]\n",
    "df_houses[\"year_built\"] = [i[0] for i in df_houses[\"year_built\"]]\n",
    "\n",
    "df_houses[\"sale_price\"] = [i.split(\",\") for i in df_houses[\"sale_price\"]]\n",
    "df_houses[\"sale_price\"] = [\"\".join(i) for i in df_houses[\"sale_price\"]]\n",
    "\n",
    "df_houses[\"house_sqft\"] = [i.split(\",\") for i in df_houses[\"house_sqft\"]]\n",
    "df_houses[\"house_sqft\"] = [\"\".join(i) for i in df_houses[\"house_sqft\"]]\n",
    "\n",
    "df_houses[\"lotsize\"] = [i.split(\",\") for i in df_houses[\"lotsize\"]]\n",
    "df_houses[\"lotsize\"] = [\"\".join(i) for i in df_houses[\"lotsize\"]]"
   ]
